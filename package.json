{
  "name": "moonstone",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all develop",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "preview": "netlify deploy --build --context deploy-preview",
    "deploy": "netlify deploy --build --prod",
    "fmt": "npx npm-run-all format",
    "format": "npx prettier --ignore-path .gitignore --write .",
    "test": "npm-run-all test:*",
    "test:lint": "npm-run-all test:lint:*",
    "test:lint:js": "next lint",
    "test:format": "npx prettier --check . --ignore-path .gitignore"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@headlessui/react": "^1.4.2",
    "axios": "^0.25.0",
    "jsqr": "^1.4.0",
    "next": "^12.0.9",
    "npx": "^10.2.2",
    "react": "17.0.2",
    "react-cool-inview": "^2.0.8",
    "react-dom": "17.0.2",
    "react-map-gl": "^6.1.17",
    "react-mapbox-gl": "^5.1.1",
    "react-p5": "^1.3.21",
    "react-qr-reader": "^2.2.1",
    "react-reveal": "^1.2.2",
    "sharp": "^0.30.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.1",
    "netlify-cli": "^8.16.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "tailwindcss": "^3.0.7"
  }
}
